{
  "name": "react-start",
  "version": "0.1.0",
  "private": true,
  "description": "react-start",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "azure-devops-extension-api": "~1.146.1",
    "azure-devops-extension-sdk": "~2.0.4",
    "azure-devops-ui": "~1.145.0",
    "bootstrap": "^4.2.1",
    "node-sass": "^7.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "vss-web-extension-sdk": "^5.141.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.7",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.1",
    "file-loader": "~2.0.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.2.1"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --open --inline --content-base src --mode development",
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/**/*.json",
    "publish-extension": "tfx extension publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
